[user]
	email = wruggian@amazon.com
	name = Will Ruggiano
[color]
	ui = auto
[core]
	pager = less -FMRiX
	excludesfile = /Users/wruggian/dev/stuff/src/WhereWruggianKeepsHisStuff/usr/.gitignore
	editor = emacsclient -c -a=
[push]
	default = simple
[alias]
	dag = log --graph --format='format:%C(yellow)%h%C(reset) %C(blue)\"%an\" <%ae>%C(reset) %C(magenta)%cr%C(reset)%C(auto)%d%C(reset)%n%s' --date-order
	br  = branch
  	ci  = commit
  	co  = checkout
  	df  = diff
  	g   = grep -I
  	lg  = log -p
  	loq = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€” %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
  	pb  = publish-branch
  	rb  = rbranch
  	rc  = rank-contributors
  	rv  = review
  	sm  = show-merges
  	st  = status
[color "branch"]
  	current = yellow reverse
  	local   = yellow
  	remote  = green
[color "diff"]
  	meta = yellow 
  	frag = magenta 
  	old  = red 
  	new  = green 

[color "status"]
  	added     = yellow
  	changed   = green
  	untracked = cyan
[amazon]
	append-cr-url = true
	pull-request-by-default = true
[credential]
	helper = !aws --profile highlander-build-readonly codecommit credential-helper $@
	UseHttpPath = true
